train_net: "models/VGG16/kitti_val/train_rcnn_multiscale_rfcn_2d5_3x3.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_fast_rcnn_multiscale_rfcn_2d5_3x3"
#debug_info: true
